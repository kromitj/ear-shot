<section class="sign-in-form-wrapper">
  <%= form_for @user do |f| %>
    <div class="form-group">
      <!-- <%= f.label :f_name, class: "landing-form-label" %> -->
      <%= f.text_field :f_name, class: "form-control", :placeholder => "First Name", :style => "text-align: Center" %><br>

      <!--<%= f.label :l_name, class: "landing-form-label" %>-->
      <%= f.text_field :l_name, class: "form-control", :placeholder => "Last Name", :style => "text-align: Center" %><br>
      <!--<%= f.label :username, class: "landing-form-label" %>-->
      <%= f.text_field :username, class: "form-control", :placeholder => "User name", :style => "text-align: Center" %><br>
      <!--<%= f.label :email, class: "landing-form-label" %>-->
      <%= f.text_field :email, class: "form-control", :placeholder => "Email", :style => "text-align: Center" %><br>
      <!--<%= f.label :password, class: "landing-form-label" %>-->
      <%= f.password_field :password, class: "form-control", :placeholder => "Password", :style => "text-align: Center" %>
    </div>
    <div class="form-group">
      <%= f.submit class: "btn btn-success center-block" %>
    </div>
  <% end %>
</section>
