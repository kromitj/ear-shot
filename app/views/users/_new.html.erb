<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :f_name %><br>

  <%= f.label :last_name %>
  <%= f.text_field :l_name %><br>

  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.label :confirm_password %>
  <%= f.password_field :password_confirmation %><br>

  <%= f.submit "Register" %>

<% end %>
