
<div class="media-left">
  <!-- <h2>Available Songs</h2> -->


  <h2>Available Songs</h2>

  <ul class="nav nav-pills">
    <li><a class="all-songs-tab" data-toggle="pill" href="#all_songs">All</a></li>
    <li><a data-toggle="pill" href="#favorite_songs">Favorites</a></li>
    <li><a data-toggle="pill" href="#last_10">Last 10 Plays</a></li>
  </ul>

  <div class="tab-content">
    <div id="all_songs" class="all-songs-wrapper tab-pane active fade"></div>
    <div id="favorite_songs" class="tab-pane fade">
      <%= render "/media/favorite_songs" %>
    </div>
    <div id="last_10" class="tab-pane fade">
      <table class="table">
        <% last_5(@user).each do |song|%>
        <tr>
          <td class="media-column"><img class="artwork-thumbnail" src="<%= song.song.artwork %>"></td>
          <td class="media-column"><%= song.song.name %></td>
          <td class="media-column"><%= song.song.artist.name %></td>

        </tr>
        <% end %>
      </table>
    </div>
  </div>
