<h1>Manage Song</h1>
<%= form_for [@song.artist, @song] do |f|%>
<%= f.text_field :name %>
<%= f.text_field :artwork %>
<%= f.submit %>
<% end %>

<%= button_to "DELETE SONG", artist_song_path(@song.artist, @song), method: :delete %>


